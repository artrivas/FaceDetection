{
  "expo": {
    "name": "pojorni-vision",
    "slug": "pojorni-vision",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "plugins":[
      [
        "expo-image-picker",
        {
          "photosPermission": "La aplicaci칩n accede a tus fotos para enviar im치genes al servidor.",
          "cameraPermission": "La aplicaci칩n accede a tu c치mara para tomar fotos y enviarlas al servidor."
        }
      ],
      [
        "expo-screen-orientation", {
          "initialOrientation": "LANDSCAPE"
